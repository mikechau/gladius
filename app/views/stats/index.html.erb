<h1>Listing stats</h1>

<table>
  <tr>
    <th>User</th>
    <th>Str</th>
    <th>Dex</th>
    <th>Con</th>
    <th>Intel</th>
    <th>Wis</th>
    <th>Cha</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stats.each do |stat| %>
  <tr>
    <td><%= stat.user_id %></td>
    <td><%= stat.str %></td>
    <td><%= stat.dex %></td>
    <td><%= stat.con %></td>
    <td><%= stat.intel %></td>
    <td><%= stat.wis %></td>
    <td><%= stat.cha %></td>
    <td><%= link_to 'Show', stat %></td>
    <td><%= link_to 'Edit', edit_stat_path(stat) %></td>
    <td><%= link_to 'Destroy', stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Stat', new_stat_path %>
